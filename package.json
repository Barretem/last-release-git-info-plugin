{
  "name": "@barretter/last-release-git-info-plugin",
  "version": "1.1.0",
  "description": "webpack plugin for create git info when build",
  "author": "https://github.com/barretem",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/barretem/last-release-git-info-plugin.git"
  },
  "keywords": [
    "webpack",
    "plugin",
    "git",
    "commit"
  ],
  "files": [
    "src",
    "utils",
    "index.js",
    "nuxt.js"
  ],
  "main": "index.js",
  "publishConfig": {
