    "stdver": "standard-version -m '[skip ci] chore(release): v%s'"
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.2",
    "standard-version": "^9.3.0"
  },
  "lint-staged": {
    "*.@(md|json)": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
